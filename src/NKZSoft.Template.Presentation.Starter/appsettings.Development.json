{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System.Net.Http": "Warning"
      }
    },
    "EnableDbLogging": false
  },
  "Kestrel": {
    "endpoints": {
      "http": {
        "url": "http://0.0.0.0:5055"
      },
      "http2": {
        "Protocols": "Http2",
        "url": "http://0.0.0.0:5056"
      }
    }
  },
  "ConnectionStrings": {
    "PostgresConnection": {
      "ConnectionString": "Host=localhost;Port=5432;Uid=postgres;Password=postgres;",
      "Database": "template_db"
    },
    "RabbitMqConnection": {
      "ConnectionString": "rabbitmq://rabbitmq:rabbitmq@localhost:5672",
    }
  },
  "Services": {
    "IdentityService": "https://localhost:5001/api/v1/"
  },
  "Cors": [
    "http://localhost:9000",
    "http://localhost:5055"
  ],
  "Swagger":{
     "Enabled": true,
     "AuthorizationEnabled": false
   }
}
